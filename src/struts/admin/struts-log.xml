<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts PUBLIC
        "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
        "http://struts.apache.org/dtds/struts-2.0.dtd">
<struts>
	
	<package name="logAdmin" extends="admin" namespace="/admin/log">
		<action name="log" class="com.game.util.admin.log.action.LogAction" method="allLogs">
			<result name="logList">/WEB-INF/admin/log/logList.jsp</result>
			<result name="checkLog">/WEB-INF/admin/log/checkLog.jsp</result>
			<result name="success" type="redirect">/admin/log/log.shtml</result>
		</action>
	</package>
</struts>
 