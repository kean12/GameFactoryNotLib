<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class
        name="com.game.util.domain.BizKind"
        table="JD_BIZKIND"
    >
	    
	    <id name="id" column="ID" type="java.lang.Long">
	        <generator class="native">
	      		<param name="sequence">ORACLE_SEQ</param>
			</generator>
	    </id>
	    
        <property
            name="kindName"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="KIND_NAME"
            length="32"
            not-null="false"
            unique="false"
        />
        
        <property
            name="orderIndex"
            type="java.lang.Integer"
            update="true"
            insert="true"
            access="property"
            column="ORDER_INDEX"
            not-null="false"
            unique="false"
        />
        
        <property
            name="toleration"
            type="java.lang.Integer"
            update="true"
            insert="true"
            access="property"
            column="TOLERATION"
            not-null="false"
            unique="false"
        />
        
        <property
            name="remark"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="REMARK"
            length="255"
            not-null="false"
            unique="false"
        />
        
        <property
            name="isUse"
            type="java.lang.Integer"
            update="true"
            insert="true"
            access="property"
            column="IS_USE"
            not-null="false"
            unique="false"
        />
        
        <property
            name="unit"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="UNIT"
            length="8"
            not-null="false"
            unique="false"
        />
        
        <property
            name="tradeTime"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="TRADE_TIME"
            length="32"
            not-null="false"
            unique="false"
        />
        
        <property
            name="tradeType"
            type="java.lang.Integer"
            update="true"
            insert="true"
            access="property"
            column="TRADE_TYPE"
            not-null="false"
            unique="false"
        />
        
	</class>
</hibernate-mapping>
