<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class
        name="com.game.util.domain.Credit"
        table="JD_CREDIT"
    >
	    
	    <id name="id" column="ID" type="java.lang.Long">
	        <generator class="native">
	      		<param name="sequence">ORACLE_SEQ</param>
			</generator>
	    </id>
	    
        <property
            name="grade"
            type="java.lang.Integer"
            update="true"
            insert="true"
            access="property"
            column="GRADE"
            not-null="false"
            unique="false"
        />
        
        <property
            name="lower"
            type="java.lang.Integer"
            update="true"
            insert="true"
            access="property"
            column="LOWER"
            not-null="false"
            unique="false"
        />
        
        <property
            name="upper"
            type="java.lang.Integer"
            update="true"
            insert="true"
            access="property"
            column="UPPER"
            not-null="false"
            unique="false"
        />
        
        <property
            name="type"
            type="java.lang.Integer"
            update="true"
            insert="true"
            access="property"
            column="TYPE"
            not-null="false"
            unique="false"
        />
        
        
        </class>
</hibernate-mapping>
